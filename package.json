{
  "name": "my-apollo-app",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@types/prop-types": "^15.5.3",
    "apollo-boost": "^0.1.10",
    "graphql": "^0.13.2",
    "graphql-tag": "^2.9.2",
    "react": "^16.4.1",
    "react-apollo": "^2.1.7",
    "react-dom": "^16.4.1",
    "react-scripts-ts": "2.16.0"
  },
  "scripts": {
    "start": "react-scripts-ts start",
    "build": "react-scripts-ts build",
    "test": "react-scripts-ts test --env=jsdom",
    "eject": "react-scripts-ts eject",
    "prettier": "prettier --write \"./**/*.{js,jsx,ts*,md,graphql,json}\"",
    "schema": "apollo-codegen introspect-schema https://mpjk0plp9.lp.gql.zone/graphql --output ./src/schema.json",
    "types": "apollo-codegen generate ./src/**/queries.ts --addTypename --schema ./src/schema.json --target typescript --output ./src/__generated__/types.ts"
  },
  "devDependencies": {
    "@types/jest": "^23.1.3",
    "@types/node": "^10.5.0",
    "@types/react": "^16.4.4",
    "@types/react-dom": "^16.0.6",
    "apollo-codegen": "^0.20.1",
    "prettier": "^1.13.6",
    "typescript": "^2.9.2"
  }
}
